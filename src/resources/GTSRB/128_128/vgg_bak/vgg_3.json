{
  "mode": "AttackBase",
  "gpu": "3",
  "img_size": 128,
  "parentPath": "data_128_128",
  "train": {
    "trainingSet": "GTSRB",
    "namesOfModels": ["VGG19"],
    "freezeEpochs": 2,
    "unfreezeEpochs": 2,
    "trainingVerbose": true,
    "numberOfModels": [3,4],
    "savedModelPath":"data_128_128/GTSRB/GTSRB_models",
    "bestParamsJSONPath":"data_128_128/GTSRB/best_params",
    "dataAugmentation":{
      "channel_shift_range": 10.0,
      "height_shift_range": 0.1,
      "shear_range": 0.1,
      "width_shift_range": 0.1,
      "zoom_range": 0.1,
      "rotation_range": 20
    },
    "kFold_n_splits": 5
  },
  "attack":{
    "averageMode": "macro",
    "numberOfAdvExamples":6000,
    "pathToCreate":"data_128_128/GTSRB/attacks/{nameOfModel}/model_{numberOfModel}",
    "advPredPath":"data_128_128/GTSRB/attacks/{nameOfModel}/model_{numberOfModel}/{savedFilePath}",
    "advExamplesDSPath":"data_128_128/GTSRB/attacks/{nameOfModel}/model_{numberOfModel}/{dsSavedFilePath}",
    "advExamplesNoiseDSPath":"data_128_128/GTSRB/attacks/{nameOfModel}/model_{numberOfModel}/{noiseDsSavedFilePath}",
    "attackers": [
      {
        "attackerName": "PGD",
        "params":{
          "batch_size": 20,
          "max_iter": 150,
          "eps": 2.5,
          "num_random_init": 3
        },
        "savedFilePath":"(PGD)_(max_iter-150_eps-250e-2_num_random_init-3).npz",
        "dsSavedFilePath":"(PGD)_(max_iter-150_eps-250e-2_num_random_init-3)_ds",
        "noiseDsSavedFilePath":"(PGD)_(max_iter-150_eps-250e-2_num_random_init-3)_noise_ds"
      },
      {
        "attackerName": "deepFool",
        "params":{
          "batch_size": 20,
          "max_iter": 40,
          "epsilon": 4e-4
        },
        "savedFilePath":"(deepFool)_(max_iter-40_epsilon-4e-4).npz",
        "dsSavedFilePath":"(deepFool)_(max_iter-40_epsilon-4e-4)_ds",
        "noiseDsSavedFilePath":"(deepFool)_(max_iter-40_epsilon-4e-4)_noise_ds"
      },
      {
        "attackerName": "FGSM",
        "params":{
          "batch_size": 20,
          "eps": 5.0,
          "num_random_init": 3,
          "minimal": true
        },
        "savedFilePath":"(FGSM)_(eps-50e-1_num_random_init-3_minimal-true).npz",
        "dsSavedFilePath":"(FGSM)_(eps-50e-1_num_random_init-3_minimal-true)_ds",
        "noiseDsSavedFilePath":"(FGSM)_(eps-50e-1_num_random_init-3_minimal-true)_noise_ds"
      }
    ]
  },
  "denoisers":{
    "resolution": [128, 128, 3],
    "numberOfTargetModels": 1,
    "denoiserNames":["ae","unet","vae"],
    "noiseLevels":[3e-2, 0, 1e-3],
    "mode": "individual",
    "savedDenoiserPath":"data_128_128/denoisers/GTSRB/noiseLevel_{noiseLevel}/{denoiserName}",
    "bestDenoiserGivenNoiseLevel":"data_128_128/denoisers/GTSRB"
  },
  "result": {
    "savedJSONPath": "data_128_128/GTSRB/classifierBaseLineReports/report.json"
  }
}